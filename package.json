{
  "name": "react-webpack-starterkit",
  "version": "1.0.0",
  "description": "A extremely basic React/Webpack starter kit. Comes with the bare necesseties required to run React with WebPack as the module loader.",
  "main": "main.js",
  "scripts": {
    "dev": "webpack-dev-server --open --devtool eval --progress --colors --content-base build",
    "build:dev": "webpack",
    "build:prod": "webpack -p",
    "start": "webpack-dev-server --open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ravitejadaggupati/react-webpack-starterkit.git"
  },
  "keywords": [
