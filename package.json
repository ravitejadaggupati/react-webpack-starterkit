  "bugs": {
    "url": "https://github.com/ravitejadaggupati/react-webpack-starterkit/issues"
  },
  "homepage": "https://github.com/ravitejadaggupati/react-webpack-starterkit#readme",
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "html-webpack-plugin": "^2.28.0",
